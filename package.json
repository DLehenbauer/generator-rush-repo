{
  "name": "generator-rush-repo",
  "version": "0.0.1",
  "description": "A yeoman generator for Microsoft Rush monorepos",
  "keywords": [
    "yeoman-generator"
  ],
  "repository": "DLehenbauer/generator-rush-repo",
  "license": "MIT",
  "author": "Daniel Lehenbauer",
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "scripts": {
    "check": "npm ls --global --depth 0",
    "link": "npm install && npm link",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test": "rm -rf test; mkdir test && npm run link && cd test && yo rush-repo && rush lint && rush test && rush build",
    "unlink": "npm rm --global generator-rush-monorepo"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "git-remote-origin-url": "^3.1.0",
    "yeoman-generator": "^4.11.0"
  },
  "devDependencies": {
    "prettier": "^2.2.1"
  }
}
